body{
  display:flex;
  justify-content:center;
  align-items:center;
  height:100vh;
  background:#000;
}

.data-ring{
  width:150px;
  height:150px;
  border:6px dashed #0ff;
  border-radius:50%;
  animation:rotate 3s linear infinite;
}

@keyframes rotate{
  from{transform:rotate(0deg);}
  to{transform:rotate(360deg);}
}
